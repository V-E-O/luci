msgid "name"
msgstr "服务类型名称"

msgid "percent_bandwidth"
msgstr "所占带宽百分比"

msgid "percent of total bandwidth to use"
msgstr "允许占用全部带宽的百分比"

msgid "min_bandwidth"
msgstr "最小保证带宽"

msgid "min bandwidth usage in absolute speed (kbit/s)"
msgstr "最小保证网速(kbit/s)"

msgid "max_bandwidth"
msgstr "最大带宽"

msgid "max bandwidth usage in absolute speed (kbit/s)"
msgstr "最大允许网速(kbit/s)"

msgid "minRTT"
msgstr "最小往返延时"

msgid "Add"
msgstr "添加"

msgid "upload_rule"
msgstr "上传规则"

msgid "class"
msgstr "规则类型"

msgid "<abbr title=\"name of bandwidth class to use if rule matches, this is required in each rule section\">Help</abbr>"
msgstr "<abbr title=\"满足规则时使用的服务类型名称，该项对每条规则都是必选的\">帮助</abbr>"

msgid "test_order"
msgstr "测试顺序"

msgid "<abbr title=\"an integer that specifies the order in which the rule should be checked for a match (lower numbers are checked first)\">Help</abbr>"
msgstr "<abbr title=\"该项是一个整数，用来定义进行规则匹配时的先后顺序 (较小数字表示规则会被最先匹配)\">帮助</abbr>"

msgid "proto"
msgstr "协议"

msgid "<abbr title=\"check that packet has this protocol (tcp, udp, both)\">Help</abbr>"
msgstr "<abbr title=\"检测数据包协议 (TCP, UDP, 二者均有)\">帮助</abbr>"

msgid "source ip"
msgstr "来源IP地址"

msgid "<abbr title=\"check that packet has this source ip, can optionally have /[mask] after it (see -s option in iptables man page)\">Help</abbr>"
msgstr "<abbr title=\"检测数据包来源IP，在其后可接 /[mask] 形式 (参见 iptables 手册页中的 -s 选项)\">帮助</abbr>"

msgid "destination ip"
msgstr "目标IP地址"

msgid "<abbr title=\"check that packet has this destination ip, can optionally have /[mask] after it (see -d option in iptables man page)\">Help</abbr>"
msgstr "<abbr title=\"检测数据包目标IP，在其后可接 /[mask] 形式 (参见 iptables 手册页中的 -d 选项)\">帮助</abbr>"

msgid "destination port range"
msgstr "目标端口范围"

msgid "<abbr title=\"check that packet has this destination port or in this port range. i.e. 80-443\">Help</abbr>"
msgstr "<abbr title=\"检测数据包目标端口或端口范围，端口范围形如：80-443\">帮助</abbr>"

msgid "source port range"
msgstr "来源端口范围"

msgid "<abbr title=\"check that packet has this source port or in this port range. i.e. 80-443\">Help</abbr>"
msgstr "<abbr title=\"检测数据包来源端口或端口范围，端口范围形如：80-443\">帮助</abbr>"

msgid "min_pkt_size"
msgstr "数据包最小字节数"

msgid "<abbr title=\"check that packet is at least this size (in bytes)\">Help</abbr>"
msgstr "<abbr title=\"检测数据包的最小尺寸 (单位为字节)\">帮助</abbr>"

msgid "max_pkt_size"
msgstr "数据包最大字节数"

msgid "<abbr title=\"check that packet is no larger than this size (in bytes)\">Help</abbr>"
msgstr "<abbr title=\"检测数据包的最大尺寸 (单位为字节)\">帮助</abbr>"

msgid "connbytes_kbyte"
msgstr "连接字节数"

msgid "<abbr title=\"In kbyte\">Help</abbr>"
msgstr "<abbr title=\"单位为千字节\">帮助</abbr>"

msgid "ipp2p"
msgstr "IPP2P"

msgid "<abbr title=\"check whether packet matches ipp2p specification (used to recognize p2p protocols),ipp2p or all will match any of the specified p2p protocols, you can also specifically match any protocol listed in the documentation here: http://ipp2p.org/docu_en.html\">Help</abbr>"
msgstr "<abbr title=\"检测数据包是否满足 IPP2P 规则 (用来辨识 P2P 协议)，ipp2p 或者 all 选项会匹配任何一种特定 P2P 协议，你也可以使用文档中定义的任何协议规则，文档参见：http://ipp2p.org/docu_en.html\">帮助</abbr>"

